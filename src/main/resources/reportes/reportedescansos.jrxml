	<queryString>
		<![CDATA[select desc_est,
(SER_APE_PAT ||' '|| SER_APE_MAT ||' '|| SER_NOM) as NOMBRE,
ID_SERV,
CITT,
FECHA_INICIO,
FECHA_FIN,
SER_COD,
TIPO_LIC from (select desc_est,
SER_APE_PAT,
SER_APE_MAT,
SER_NOM,
ID_SERV,
CITT,
FECHA_INICIO,
FECHA_FIN,
SER_COD,
TIPO_LIC from DATAPERLIQU.desc_medicos dm,DATAPERSUEL.lista_servidor se
where (to_char(fecha_inicio, 'mm')=$P{mes} or to_char(fecha_fin, 'mm')=$P{mes})
and (to_char(fecha_inicio, 'yyyy')=$P{anio} or to_char(fecha_fin, 'yyyy')=$P{anio})
and trim(dm.id_serv)=trim(se.ser_cod)
and se.NUM_SEREST=1
and $X{IN,id_serv,vectorSeleccionados})]]>
	</queryString>
