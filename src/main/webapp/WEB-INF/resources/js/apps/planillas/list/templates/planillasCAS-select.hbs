<label class="col-md-4 control-label" for="select-planillasCAS">Planilla </label>

<div class="col-md-8">
    <select id="select-planillasCAS" class="form-control">
        {{#each items}}
            <option value="{{numero}}">{{numero}}</option>
        {{else}}
            <option value="0" selected>Sin planillas</option>
        {{/each}}
    </select>
</div>
