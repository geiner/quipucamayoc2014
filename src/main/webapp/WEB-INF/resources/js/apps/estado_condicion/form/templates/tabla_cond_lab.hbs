<table id="table-cond-lab" class="table table-bordered table-hover" style="font-size: 12px;">
    <thead class="header" style="color: #040404;background-color:#f5f5f5">
    <tr>
        <th >Nº Registro</th>
        <th >Nº Resolucion</th>
        <th >Estado Laboral</th>
        <th>Tipo Genérico</th>
        <th>Tipo</th>
        <th>Categoria</th>
    </tr>
    </thead>
    <tbody id="">
        {{#each items}}
        <tr class="">
            <td>{{numReg}}</td>
            <td>{{numResol}}</td>
            <td>{{estado}}</td>
            <td>{{descGen}}</td>
            <td>{{tipoServicio}}</td>
            <td>{{categoria}}</td>
        </tr>
        {{else}}
        <td colspan="6">No existen registros para este Trabajador</td>
        {{/each}}
    </tbody>
</table>
