    <tr>
        <th>NÂº Resol.</th>
        <th>Fecha</th>
        <th>Tipo </th>
        <th>Motivo</th>
        <th>Descripcion</th>
    </tr>
    </thead>
    <tbody class="body-table" id="fam_content">
    {{#each items}}
        <tr id="{{resid}}" class="">
