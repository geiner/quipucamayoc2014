            <td>{{cargfamdir}}</td>
            <td>{{cargfampar}}</td>
            <td>{{cargfamdoc}}</td>
            <td>{{cargfamnumdoc}}</td>
            <td>{{cargfamsex}}</td>
            <td><a class="btn btn-warning btn-xs" id="add-observacion"><span class="glyphicon glyphicon-pencil"></span></a> {{numeroObservaciones}}</td>
            <td><a class="btn  btn-danger btn-xs"  id="modalBV" data-toggle="modal"  data-target="#serv-table-modal7"><span  class="glyphicon glyphicon-trash"></span></a></td>
        </tr>
        {{else}}
        <td colspan="8">No existen familiares para este trabajador</td>
        {{/each}}
    </tbody>
</table>
