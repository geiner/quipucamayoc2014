    <thead class="header" style="color: #040404;background-color:#f5f5f5">
    <tr>
        <th>Nombre y Apellido</th>
        <th>Domicilio</th>
        <th>Tipo de Parentesco</th>
        <th>Tipo de Documento</th>
        <th>NÂº de Documento</th>
        <th>Sexo</th>
        <th>Editar</th>
        <th>Eliminar</th>
    </tr>
    </thead>
    <tbody id="fam_content">

        {{#each items}}
        <tr id="" class="" dataid='{{cargfamsec}}' datatel='{{cargfamtel}}' datatipbenf='{{tipbeneficio}}' dataresol='{{codresol}}' datatitular='{{titularcuenta}}' datatipago='{{tipopago}}' dataparent='{{cargfamidpar}}' datafechnac='{{cargfamfechnac}}' datanumcuen='{{numcuenta}}' datadocum='{{cargfamiddoc}}' databenf='{{cargfamben}}' datadep='{{cargfamdep}}' dataestcivil='{{cargfamrestciv}}' datasegur='{{cargfamnumessal}}'>
