<table id="table-descansos-servidor" class="table table-bordered table-hover" style="font-size: 12px;">
    <thead class="header" style="color: #040404;background-color:#f5f5f5">
    <tr>
        <th>CITT</th>
        <th>Tipo Licencia</th>
        <th>Fecha de Inicio</th>
        <th>Fecha fin</th>

        <th>Tiempo</th>

        <!--<th>Editar</th>-->
        <th>Eliminar</th>
    </tr>
    </thead>
    <tbody id="fam_content">
    {{#each items}}
        <tr id="{{id_desc_med}}" class="">
            <td>{{citt}}</td>
            <td>{{tipo_lic}}</td>
            <td>{{f_inicio}}</td>
            <td>{{f_fin}}</td>

            <td>{{tiempo}}</td>
            <!--<td><a id="edit_desc" class="btn btn-warning btn-xs"><span class="glyphicon glyphicon-pencil"></span></a></td>-->
            <td><a id="modalAV" class="btn  btn-danger btn-xs" data-toggle="modal" data-target="#show_eliminar"><span class="glyphicon glyphicon-trash"></span></a></td>
        </tr>
    {{else}}
        <td colspan="6">No existen estudios para este Servidor</td>
    {{/each}}
    </tbody>
</table>
