<table id="table-descansos-servidor" class="table table-bordered table-hover" style="font-size: 12px;">
    <thead class="header" style="color: #040404;background-color:#f5f5f5">
    <tr>
        <th>CITT</th>
        <th>Fecha de Inicio</th>
        <th>Fecha fin</th>
        <th>Tipo Licencia</th>
    </tr>
    </thead>
    <tbody id="fam_content">
    {{#each items}}
        <tr id="{{id_desc_med}}" class="">
            <td>{{citt}}</td>
            <td>{{f_inicio}}</td>
            <td>{{f_fin}}</td>
            <td>{{tipo_lic}}</td>
        </tr>
    {{else}}
        <td colspan="6">No existen estudios para este Servidor</td>
    {{/each}}
    </tbody>
</table>
